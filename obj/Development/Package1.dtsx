      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{4323D3A4-DB94-4FA8-929B-34A4E64D937F}"
          SQLTask:SqlStatementSource="&#xA;BEGIN TRY&#xA; &#xA;    BEGIN TRANSACTION;&#xA; &#xA;    -- Merge statement to insert or update sales in the Fact_Sales table&#xA;&#xA;    MERGE INTO [dbo].[Fact_Sales] AS Target&#xA;&#xA;    USING (&#xA;&#xA;        SELECT DISTINCT&#xA;&#xA;            ups.[Total APB],&#xA;&#xA;            ups.[Total APB%],&#xA;&#xA;            ups.[Effort],&#xA;&#xA;            ups.[Total Taxes],&#xA;&#xA;            ups.[EXTRA],&#xA;&#xA;            ups.[% / total],&#xA;&#xA;            ups.[% / subt#],&#xA;&#xA;            ups.[Other#Contrib],&#xA;&#xA;            dd.[DealerKey],&#xA;&#xA;            ups.[Prime de volume(%)],&#xA;&#xA;            ups.[Advertised],&#xA;&#xA;            ups.[Click],&#xA;&#xA;            ups.[Attractivité Pub],&#xA;&#xA;            ups.[FME Par Brand],&#xA;&#xA;            ups.[List Price],&#xA;&#xA;            ups.[Promo Price],&#xA;&#xA;            CAST(ups.[Year] AS VARCHAR(4)) + '-' + RIGHT('0' + CAST(ups.[Month] AS VARCHAR(2)), 2) + '-01' AS DateKey,&#xA;&#xA;            dc.[CountryKey],&#xA;&#xA;            dci.[CarKey],&#xA;&#xA;            dp.[PromoTypeKey],&#xA;&#xA;            dpt.[PromoTypeKey] AS PromoTypeOKey,&#xA;&#xA;            ups.[PromoAmount],&#xA;&#xA;            ups.[PromoAmountO]&#xA;&#xA;        FROM &#xA;&#xA;            [ODS].[dbo].[Unpivot_TEMP_Sales] ups&#xA;&#xA;        LEFT JOIN [DataWH].[dbo].[Dim_Date] dte&#xA;&#xA;            ON CAST(ups.[Year] AS VARCHAR(4)) + '-' + RIGHT('0' + CAST(ups.[Month] AS VARCHAR(2)), 2) + '-01' = dte.[DateKey]&#xA;&#xA;        LEFT JOIN [DataWH].[dbo].[Dim_PromoType] dp &#xA;&#xA;            ON ups.[PromoType] = dp.[PromoType]&#xA;&#xA;        LEFT JOIN [DataWH].[dbo].[Dim_PromoType] dpt &#xA;&#xA;            ON ups.[PromoTypeO] = dpt.[PromoType]&#xA;&#xA;        LEFT JOIN [DataWH].[dbo].[Dim_Country] dc &#xA;&#xA;            ON ups.[CountryKey] = dc.[CountryKey]&#xA;&#xA;        LEFT JOIN [DataWH].[dbo].[Dim_CarInfo] dci &#xA;&#xA;            ON ups.[CarKey] = dci.[CarKey]&#xA;&#xA;        LEFT JOIN [DataWH].[dbo].[Dim_Dealer] dd &#xA;&#xA;            ON ups.[DealerKey] = dd.[DealerKey]&#xA;&#xA;    ) AS Source&#xA;&#xA;    ON Target.[CarKey] = Source.[CarKey]&#xA;&#xA;       AND Target.[PromoTypeKey] = Source.[PromoTypeKey]&#xA;&#xA;       AND Target.[PromoTypeOKey] = Source.[PromoTypeOKey]&#xA;&#xA;       AND Target.[DealerKey] = Source.[DealerKey]&#xA;&#xA;       AND Target.[CountryKey] = Source.[CountryKey]&#xA;&#xA;       AND Target.[DateKey] = Source.[DateKey]&#xA; &#xA;    WHEN MATCHED THEN&#xA;&#xA;        UPDATE SET&#xA;&#xA;            Target.[Total APB] = Source.[Total APB],&#xA;&#xA;            Target.[Total APB%] = Source.[Total APB%],&#xA;&#xA;            Target.[Effort] = Source.[Effort],&#xA;&#xA;            Target.[Total Taxes] = Source.[Total Taxes],&#xA;&#xA;            Target.[EXTRA] = Source.[EXTRA],&#xA;&#xA;            Target.[% / total] = Source.[% / total],&#xA;&#xA;            Target.[% / subt#] = Source.[% / subt#],&#xA;&#xA;            Target.[Other#Contrib] = Source.[Other#Contrib],&#xA;&#xA;            Target.[Prime de volume(%)] = Source.[Prime de volume(%)],&#xA;&#xA;            Target.[Advertised] = Source.[Advertised],&#xA;&#xA;            Target.[Click] = Source.[Click],&#xA;&#xA;            Target.[Attractivité Pub] = Source.[Attractivité Pub],&#xA;&#xA;            Target.[FME Par Brand] = Source.[FME Par Brand],&#xA;&#xA;            Target.[List Price] = Source.[List Price],&#xA;&#xA;            Target.[Promo Price] = Source.[Promo Price],&#xA;&#xA;            Target.[PromoAmount] = Source.[PromoAmount],&#xA;&#xA;            Target.[PromoAmountO] = Source.[PromoAmountO],&#xA;&#xA;            Target.[EndDate] = GETDATE()&#xA; &#xA;    WHEN NOT MATCHED BY TARGET THEN&#xA;&#xA;        INSERT (&#xA;&#xA;            [CountryKey], [CarKey], [DealerKey], [DateKey], [Total APB], [Total APB%], [Effort], &#xA;&#xA;            [Total Taxes], [EXTRA], [% / total], [% / subt#], [Other#Contrib], &#xA;&#xA;            [Prime de volume(%)],  [Advertised], [Click], [FME Par Brand], &#xA;&#xA;            [List Price], [Promo Price], [Attractivité Pub], [PromoTypeKey], [PromoAmount], &#xA;&#xA;            [PromoTypeOKey], [PromoAmountO], [StartDate]&#xA;&#xA;        )&#xA;&#xA;        VALUES (&#xA;&#xA;            Source.[CountryKey], Source.[CarKey], Source.[DealerKey], Source.[DateKey], Source.[Total APB], &#xA;&#xA;            Source.[Total APB%], Source.[Effort], Source.[Total Taxes], Source.[EXTRA], &#xA;&#xA;            Source.[% / total], Source.[% / subt#], Source.[Other#Contrib], Source.[Prime de volume(%)], &#xA;&#xA;            Source.[Advertised], Source.[Click], Source.[FME Par Brand], &#xA;&#xA;            Source.[List Price], Source.[Promo Price], Source.[Attractivité Pub], &#xA;&#xA;            Source.[PromoTypeKey], Source.[PromoAmount], Source.[PromoTypeOKey], Source.[PromoAmountO], &#xA;&#xA;            GETDATE()&#xA;&#xA;        );&#xA; &#xA;    -- Log the result for debugging&#xA;&#xA;    SELECT 'Rows affected: ' + CAST(@@ROWCOUNT AS VARCHAR(10)) AS RowsAffected;&#xA; &#xA;    COMMIT TRANSACTION;&#xA; &#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;&#xA;    IF @@TRANCOUNT &gt; 0&#xA;&#xA;        ROLLBACK TRANSACTION;&#xA; &#xA;    DECLARE @ErrMsg NVARCHAR(4000), @ErrSeverity INT;&#xA;&#xA;    SELECT @ErrMsg = ERROR_MESSAGE(),&#xA;&#xA;           @ErrSeverity = ERROR_SEVERITY();&#xA;&#xA;    RAISERROR(@ErrMsg, @ErrSeverity, 1);&#xA;&#xA;END CATCH;&#xA;&#xA; " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
