          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{4323D3A4-DB94-4FA8-929B-34A4E64D937F}"
              SQLTask:SqlStatementSource="BEGIN TRY&#xA;    BEGIN TRANSACTION;&#xA;&#xA;    MERGE INTO [dbo].[Dim_Brand] AS Target&#xA;    USING (&#xA;        SELECT DISTINCT&#xA;            cd.[BrandKey],&#xA;            cd.[Brand] AS BrandName&#xA;        FROM [ODS].[dbo].[TEMP_Brand] cd&#xA;        WHERE cd.[BrandKey] IS NOT NULL&#xA;          AND cd.[Brand] IS NOT NULL&#xA;    ) AS Source&#xA;    ON Target.[BrandKey] = Source.[BrandKey]&#xA;    WHEN MATCHED AND (Target.[BrandName] &lt;&gt; Source.[BrandName] ) THEN&#xA;        UPDATE SET&#xA;            Target.[BrandName] = Source.[BrandName],&#xA;            Target.[EndDate] = GETDATE()&#xA;    WHEN NOT MATCHED BY TARGET THEN&#xA;        INSERT ([BrandKey], [BrandName], [StartDate])&#xA;        VALUES (Source.[BrandKey], Source.[BrandName],GETDATE());&#xA;    &#xA;&#xA;    COMMIT TRANSACTION;&#xA;END TRY&#xA;BEGIN CATCH&#xA;    IF @@TRANCOUNT &gt; 0&#xA;        ROLLBACK TRANSACTION;&#xA;&#xA;    DECLARE @ErrMsg NVARCHAR(4000), @ErrSeverity INT;&#xA;    SELECT @ErrMsg = ERROR_MESSAGE(),&#xA;           @ErrSeverity = ERROR_SEVERITY();&#xA;    RAISERROR(@ErrMsg, @ErrSeverity, 1);&#xA;END CATCH;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
